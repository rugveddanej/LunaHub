-- Function to load and execute a script from a given URL
local function loadScriptFromGitHub(url)
    local success, scriptContent = pcall(function()
        return game:HttpGet(url)
    end)

    if success then
        local loadedFunction, loadError = loadstring(scriptContent)
        if loadedFunction then
            local executeSuccess, executeError = pcall(loadedFunction)
            if not executeSuccess then
                warn("Error executing loaded script: ", executeError)
            end
        else
            warn("Error loading script: ", loadError)
        end
    else
        warn("Error fetching script from URL:", url)
    end
end

-- Table with game IDs and corresponding script URLs
local gameScripts = {
    [11468034852] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/mugen-train",
    [11468075017] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/ouwigahara",
    [13883059853] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/map2p",
    [13883279773] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/map1p",
    [17387475546] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/map1n",
    [17387482786] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/map2n",
    [13489082242] = "https://raw.githubusercontent.com/rugveddanej/LunaHub/main/Project_Slayer/trade"
}

-- Get the current game ID
local currentGameId = game.PlaceId

-- Check if there's a script for the current game
local scriptUrl = gameScripts[currentGameId]
if scriptUrl then
    -- Load and execute the script
    loadScriptFromGitHub(scriptUrl)
else
    warn("No script found for this game.")
end